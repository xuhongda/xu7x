<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!--mybatis逆向工程配置-->
    <!--
    targetRuntime="MyBatis3"：复杂版的CRUD；（可以带复杂条件的）
    targetRuntime="MyBatis3Simple"：简单版的CRUD；（只能按主键进行CRUD）
     -->
    <context id="DB2Tables" targetRuntime="MyBatis3">

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- 配置数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://129.204.79.247:3306/mybatis_springboot"
                        userId="root"
                        password="7777">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- javaModelGenerator:指定javaBean生成的地方 -->
        <javaModelGenerator
                targetPackage="pojo"
                targetProject="D:\xucode\my\xu7x\xu7x-model\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!--sqlMapGenerator：指定生成xml的地方  -->
        <sqlMapGenerator targetPackage="mybatis.mapper"
                         targetProject="D:\xucode\my\xu7x\xu7x-dao\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!-- javaClientGenerator:指定dao接口生成的地方 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.xu.xu7x.mapper"
                             targetProject="D:\xucode\my\xu7x\xu7x-dao\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!-- 指定需要逆向的表格:
            默认是按照驼峰命名法进行大小写转换； Txxxx；快速的区分javaBean的类型（DO、VO）
            DO：Database Object（数据库映射的对象）
            VO：【View Object】【Value Object】（封装给页面需要显示的数据）
         -->
        <table tableName="xu7x_content"/>
        <table tableName="xu7x_index"/>

